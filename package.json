{
  "name": "near-eliza-starter",
  "version": "0.1.9",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "start": "tsc --noEmit && node --loader ts-node/esm src/index.ts",
    "start:client": "pnpm --dir client dev",
    "clean": "./scripts/clean.sh",
    "start:service:all": "pm2 start pnpm --name=\"all\" --restart-delay=3000 --max-restarts=10 -- run start:all",
    "stop:service:all": "pm2 stop all",
    "postinstall": "pnpm rebuild better-sqlite3 && bash scripts/multichain-tools/update-rpc.sh"
  },
  "dependencies": {
    "@elizaos/adapter-postgres": "0.1.9",
    "@elizaos/adapter-sqlite": "0.1.9",
    "@elizaos/client-auto": "0.1.9",
    "@elizaos/client-direct": "0.1.9",
    "@elizaos/client-discord": "0.1.9",
    "@elizaos/client-telegram": "0.1.9",
    "@elizaos/client-twitter": "0.1.9",
    "@elizaos/core": "npm:@near-agent/elizaos-core@0.1.9-patch.2",
    "@elizaos/plugin-bootstrap": "0.1.9",
    "@elizaos/plugin-image-generation": "0.1.9",
    "@elizaos/plugin-multichain": "npm:@near-agent/elizaos-plugin-multichain",
    "@elizaos/plugin-near": "npm:@near-agent/elizaos-plugin-near@0.1.9-patch.2",
    "@elizaos/plugin-node": "0.1.9",
    "@tavily/core": "0.0.2",
    "amqplib": "0.10.5",
    "better-sqlite3": "11.5.0",
    "fs": "0.0.1-security",
    "multichain-tools": "4.0.0",
    "net": "1.0.2",
    "path": "0.12.7",
    "readline": "1.3.0",
    "url": "0.11.4",
    "ws": "8.18.0",
    "yargs": "17.7.2"
  },
  "engines": {
    "node": ">=22"
  },
  "pnpm": {
    "overrides": {
      "@elizaos/core@0.1.9": "npm:@near-agent/elizaos-core@0.1.9-patch.2",
      "onnxruntime-node": "1.20.0"
    },
    "onlyBuiltDependencies": [
      "better-sqlite3"
    ]
  },
  "devDependencies": {
    "pm2": "5.4.3",
    "ts-node": "10.9.2",
    "tsup": "8.3.5",
    "typescript": "5.6.3"
  }
}
